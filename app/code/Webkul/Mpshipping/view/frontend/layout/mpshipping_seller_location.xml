<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <script src="Webkul_Mpshipping/js/distance.js"/>
    </head>
    <body>
        <referenceContainer name="content">
        <block class="Magento\Framework\View\Element\Template" name="mpshipping_distance" template="Webkul_Mpshipping::addLocation.phtml" cacheable="false" before="-">
            <arguments>
                <argument name="location_model" xsi:type="object">Webkul\Mpshipping\ViewModel\LocationModel</argument>
            </arguments>
        </block>
        </referenceContainer>
    </body>
</page>
